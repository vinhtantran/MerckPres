@article{Wetlaufer2016,
    abstract = {Accurate representation of the spatial distribution of snow water equivalent (SWE) in mountainous basins is critical for furthering the understanding of snow as a water resource, especially in the Western United States. To estimate the spatial distribution and total volume of SWE over mountainous basins, previous work has either assumed uniform snow density or used simple approaches to estimate density. This study uses over 1000 direct measurements of SWE and snow depth (from which density was calculated) in sampling areas that were physiographically proportional to a large (207 km2) mountainous basin in southwest Montana. Using these data, modeled spatial distributions of density and depth were developed and combined to obtain estimates of total basin SWE. Six estimates of SWE were obtained using varying combinations of the distributed depth and density models and were compared to the average of three different models that utilized direct measurements of SWE. Models utilizing direct SWE measurements varied by approximately 1{\%} around their mean, while SWE estimates derived from combined depth and density models varied by over 14{\%} around the same mean. This study highlights the need to carefully consider the spatial variability of density when estimating SWE based on snow depth in these environments.},
    author = {Wetlaufer, Karl and Hendrikx, Jordy and Marshall, Lucy},
    doi = {10.3390/hydrology3010003},
    issn = {2306-5338},
    journal = {Hydrology},
    keywords = {SWE,density,modeling,spatial,variability},
    mendeley-groups = {Stat},
    number = {1},
    pages = {3},
    title = {{Spatial Heterogeneity of Snow Density and Its Influence on Snow Water Equivalence Estimates in a Large Mountainous Basin}},
    volume = {3},
    year = {2016}
}
@article{Boik1987,
    abstract = {Examines the robustness of the Fisher-Pitman permutation test to violation of the homogeneity requirement by comparing the size of the permutation test of equality of means to the size of the normal theory F test in small samples when variances are unequal. Normally distributed populations having equal means were assumed. The size of the permutation test was smaller than the size of the F test when the ratio of the harmonic to the arithmetic mean of the sample sizes was small and vice versa when the ratio was large. In either case, the difference between the sizes of the 2 tests was relatively small, except for extreme heterogeneity.},
    author = {Boik, Robert J.},
    doi = {10.1111/j.2044-8317.1987.tb00865.x},
    file = {:C$\backslash$:/Users/vinht/Downloads/Boik-1987-British{\_}Journal{\_}of{\_}Mathematical{\_}and{\_}Statistical{\_}Psychology (1).pdf:pdf},
    isbn = {0007-1102},
    issn = {20448317},
    journal = {British Journal of Mathematical and Statistical Psychology},
    number = {1},
    pages = {26--42},
    title = {{The Fisher‐Pitman permutation test: A non‐robust alternative to the normal theory F test when variances are heterogeneous}},
    volume = {40},
    year = {1987}
